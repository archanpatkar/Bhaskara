pg1 := {
    balance:0,
    lt:0,
    def deposit(v) {
        if v > 0 {
            this.balance = this.balance + v
            this.lt = v
        }
    },
    def withdraw(v) {
        if v <= this.balance {
            this.balance = this.balance - v
            this.lt = -v
        }
    },
    def statement() {
        this.balance |> print
        this.lt |> print
    }
}

print(pg1)
pg1.deposit(10)
pg1.withdraw(3)
pg1.statement()

